  <script src="https://use.fontawesome.com/a7ec7986bf.js"></script>
  <title>AGSD COMBAT</title>
</head>

<body>


  <div class="container">

    <div class="row">

      <header>
        <!-- BANNER -->
        <div class="row" id="banner-background">
          <!-- TITLE -->
          <div class="title">
            <h1>AGSD COMBAT Saint-Denis (93)</h1>
            <br>
            <h3 class="sousTilte">Sports de combats pour les adultes et les enfants à partir de 3 ans sur la
              ville de Saint-Denis 93200</h3>
          </div>
        </div>
      </header>



      <div class="form">

        <div class="tab-content">

          <h1>S'identifier</h1>
          <!-- SIGN IN AREA -->

          <!-- message affiché si ya une erreur lors de la validation du form -->
          <% include ../partials/messages %>
          <br>
          <form action="/users/login" method="post">

            <div class="field-wrap">
              <label>
                Email Address<span class="req">*</span>
              </label>
              <input style="width: 250px" name="emailFromFront" id="mail" type="email" required autocomplete="off" />
            </div>

            <div class="field-wrap">
              <label>
                Password<span class="req">*</span>
              </label>
              <input style="width:250px" name="passwordFromFront" type="password" id="password" required
                autocomplete="off" />
            </div>

            <!-- oublis de mot de passe -->
            <!-- <p class="forgot"><a href="#">Forgot Password?</a></p> -->

            <button class="button button-block" />SIGN IN</button>

          </form>
          <br>
          <p>Si vous n'etes pas inscrit <a href="/users/register">Inscription</a></p>


          <div id="login">

          </div>

        </div><!-- tab-content -->

      </div> <!-- /form -->

    </div>

    <!-- FOOTER -->
    <footer>
      <div class="wrapper ">
        <!-- Icons reseaux sociaux -->
        <div class="social-btns"><a class="btn facebook" href="https://www.facebook.com/agsdcombat/?ref=bookmarks"><i
              class="fa fa-facebook"></i></a>
          <a class="btn twitter" href="https://twitter.com/CombatAgsd"><i class="fa fa-twitter"></i></a>
          <a class="btn google" href="https://g.page/agsdcombat?share"><i class="fa fa-google"></i></a>
          <a class="btn insta" href="https://www.instagram.com/agsdcombat/"><i class="fa fa-instagram"></i></a>
          <a class="btn youtube" href="https://www.youtube.com/channel/UCDUTivMSGwXPmzMIGMwgWUA"><i
              class="fa fa-youtube"></i></a>
        </div>
      </div>
    </footer>

  </div>
  <!-- boutton scroll -->
  <a href="#" title="Haut de page" class="scrollup"><i class="fa fa-arrow-up"></i></a>



  <script src="/javascripts/scryptLogin.js"></script>
